<COMPONENT>
  <HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.102" STATUS="ALPHA"
    COMPATIBILITY="ALL" AUTHOR="Camptocamp" RELEASE_DATE="20070521"
    STARTABLE="true" SCHEMA_AUTO_PROPAGATE="true"
    DATA_AUTO_PROPAGATE="false">
    <SIGNATURE />
  </HEADER>
  <FAMILIES>
    <FAMILY>Geo/File/Input</FAMILY>
  </FAMILIES>
  <DOCUMENTATION>
    <URL />
  </DOCUMENTATION>
  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="1" />
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK" />
    <CONNECTOR CTYPE="COMPONENT_ERROR" />
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="RUN_IF" />
  </CONNECTORS>
  <PARAMETERS>
    <PARAMETER NAME="FORMAT_TYPE" FIELD="TEXT" NUM_ROW="0" SHOW="false"
      REQUIRED="true">
      <DEFAULT>WFS</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="WFS_URL" FIELD="TEXT" NUM_ROW="3" REQUIRED="true"
      REPOSITORY_VALUE="WFS_URL">
      <DEFAULT>"http://services.sandre.eaufrance.fr/geo/ouvrage"</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="WFS_VERSION" FIELD="CLOSED_LIST" NUM_ROW="4"
      REQUIRED="true">
      <ITEMS DEFAULT="v1_0_0">
        <ITEM NAME="v1_0_0"
          VALUE="org.geotools.wfs.protocol.Version.v1_0_0" />
        <ITEM NAME="v1_1_0"
          VALUE="org.geotools.wfs.protocol.Version.v1_1_0" />
      </ITEMS>
    </PARAMETER>
    <PARAMETER NAME="TYPE_NAME" FIELD="TEXT" NUM_ROW="4"
      REQUIRED="true">
      <DEFAULT>"REPOM"</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true"
      NUM_ROW="5">
      <TABLE>
        <COLUMN NAME="msGeometry" TYPE="id_Geometry" CUSTOM="false"
          READONLY="false" />
      </TABLE>
    </PARAMETER>
    <PARAMETER NAME="FORCE_CRS" FIELD="CHECK" REQUIRED="true"
      NUM_ROW="13" SHOW="false">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="CRS" FIELD="TEXT" NUM_ROW="13" REQUIRED="true"
      SHOW_IF="FORCE_CRS == 'true'" SHOW="false">
      <DEFAULT>"EPSG:4326"</DEFAULT>
    </PARAMETER>
  </PARAMETERS>
  <ADVANCED_PARAMETERS>
    <PARAMETER NAME="CQL" FIELD="TEXT" NUM_ROW="15" REQUIRED="true">
      <DEFAULT>""</DEFAULT>
    </PARAMETER>    
    <PARAMETER NAME="USERNAME" FIELD="TEXT" NUM_ROW="5" REQUIRED="false"
      REPOSITORY_VALUE="USERNAME">
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="PASSWORD" FIELD="TEXT" REQUIRED="false" NUM_ROW="6"
      REPOSITORY_VALUE="PASSWORD">
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="USE_METHOD" FIELD="CLOSED_LIST" NUM_ROW="4"
      REQUIRED="true">
      <ITEMS DEFAULT="USE_POST">
        <ITEM NAME="USE_POST" VALUE="USE_POST" />
        <ITEM NAME="USE_GET" VALUE="USE_GET" />
      </ITEMS>
    </PARAMETER>
    <PARAMETER NAME="TIMEOUT" FIELD="TEXT" NUM_ROW="8"
      REQUIRED="false">
      <DEFAULT>3000</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="BUFFER_SIZE" FIELD="TEXT" NUM_ROW="9"
      REQUIRED="false">
      <DEFAULT>100</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="LENIENT" FIELD="CHECK" NUM_ROW="10"
      REQUIRED="false">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="TRY_GZIP" FIELD="CHECK" NUM_ROW="11" 
      REQUIRED="false">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="IMPORT" FIELD="MEMO_IMPORT" REQUIRED="false" SHOW="false" NUM_ROW="2">
	  <DEFAULT>import org.talend.sdi.geometry.Geometry;</DEFAULT>
    </PARAMETER>
  </ADVANCED_PARAMETERS>
  <CODEGENERATION>
    <TEMPLATES INPUT="GI" OUTPUT="FF">
      <TEMPLATE NAME="GI" COMPONENT="sGeoInput">
        <LINK_TO NAME="FF" CTYPE="FLOW" />
      </TEMPLATE>
      <TEMPLATE NAME="FF" COMPONENT="sFlowForwarder" />
      <TEMPLATE_PARAM SOURCE="self.PROPERTY" TARGET="GI.PROPERTY" />
      <TEMPLATE_PARAM SOURCE="self.FORMAT_TYPE" TARGET="GI.FORMAT_TYPE" />
      <TEMPLATE_PARAM SOURCE="self.SCHEMA" TARGET="GI.SCHEMA" />
      <TEMPLATE_PARAM SOURCE="self.FORCE_CRS" TARGET="GI.FORCE_CRS" />
      <TEMPLATE_PARAM SOURCE="self.CRS" TARGET="GI.CRS" />
      <TEMPLATE_PARAM SOURCE="self.WFS_URL" TARGET="GI.WFS_URL" />
      <TEMPLATE_PARAM SOURCE="self.CQL" TARGET="GI.CQL" />
      <TEMPLATE_PARAM SOURCE="self.WFS_VERSION" TARGET="GI.WFS_VERSION" />
      <TEMPLATE_PARAM SOURCE="self.USE_METHOD" TARGET="GI.USE_METHOD" />
      <TEMPLATE_PARAM SOURCE="self.USERNAME" TARGET="GI.USERNAME" />
      <TEMPLATE_PARAM SOURCE="self.PASSWORD" TARGET="GI.PASSWORD" />
      <TEMPLATE_PARAM SOURCE="self.TYPE_NAME" TARGET="GI.TYPE_NAME" />
      <TEMPLATE_PARAM SOURCE="self.TIMEOUT" TARGET="GI.TIMEOUT" />
      <TEMPLATE_PARAM SOURCE="self.BUFFER_SIZE" TARGET="GI.BUFFER_SIZE" />
      <TEMPLATE_PARAM SOURCE="self.LENIENT" TARGET="GI.LENIENT" />
      <TEMPLATE_PARAM SOURCE="self.TRY_GZIP" TARGET="GI.TRY_GZIP" />
      <TEMPLATE_PARAM SOURCE="self.SCHEMA" TARGET="FF.SCHEMA" />
    </TEMPLATES>
  </CODEGENERATION>
  <RETURNS>
    <RETURN NAME="NB_FEATURE" TYPE="id_Integer" AVAILABILITY="AFTER" />
  </RETURNS>
</COMPONENT>
