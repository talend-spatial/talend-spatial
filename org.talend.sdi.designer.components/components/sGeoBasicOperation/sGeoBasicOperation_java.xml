<COMPONENT>
  <HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.101" STATUS="ALPHA"
    COMPATIBILITY="ALL" AUTHOR="Camptocamp" RELEASE_DATE="20070521"
    STARTABLE="true" VISIBLE="false" DATA_AUTO_PROPAGATE="false">
    <SIGNATURE></SIGNATURE>
  </HEADER>
  <FAMILIES>
    <FAMILY>Technical</FAMILY>
  </FAMILIES>
  <DOCUMENTATION>
    <URL />
  </DOCUMENTATION>
  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" />
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0" />
    <CONNECTOR CTYPE="COMPONENT_OK" />
    <CONNECTOR CTYPE="COMPONENT_ERROR" />
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="RUN_IF" />
  </CONNECTORS>
  <PARAMETERS>
    <PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true"
      NUM_ROW="1">
    </PARAMETER>
    <PARAMETER NAME="OPERATION_TYPE" FIELD="CLOSED_LIST" NUM_ROW="2">
      <ITEMS DEFAULT="LENGTH">
        <ITEM NAME="LENGTH" VALUE="LENGTH" />
        <ITEM NAME="AREA" VALUE="AREA" />
        <ITEM NAME="CENTROID" VALUE="CENTROID" />
        <ITEM NAME="POLYGONIZER" VALUE="POLYGONIZER" />
        <ITEM NAME="ENVELOPE" VALUE="ENVELOPE" />
        <ITEM NAME="BUFFER" VALUE="BUFFER" />
        <ITEM NAME="CONVEXHULL" VALUE="CONVEXHULL" />
        <ITEM NAME="DOUGLAS_PEUCKER" VALUE="DOUGLAS_PEUCKER" />
        <ITEM NAME="TOPOLOGY_PRESERVING" VALUE="TOPOLOGY_PRESERVING" />
      </ITEMS>
    </PARAMETER>
    <PARAMETER NAME="BUFFER_DISTANCE" FIELD="TEXT" REQUIRED="true"
      NUM_ROW="3" SHOW_IF="OPERATION_TYPE=='BUFFER'">
      <DEFAULT>10</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="BUFFER_END_CAP_STYLE" FIELD="CLOSED_LIST"
      NUM_ROW="3" SHOW_IF="OPERATION_TYPE=='BUFFER'">
      <ITEMS DEFAULT="CAP_ROUND">
        <ITEM NAME="CAP_ROUND" VALUE="CAP_ROUND" />
        <ITEM NAME="CAP_BUTT" VALUE="CAP_BUTT" />
        <ITEM NAME="CAP_SQUARE" VALUE="CAP_SQUARE" />
      </ITEMS>
    </PARAMETER>
    <PARAMETER NAME="BUFFER_QUANTIZATION" FIELD="TEXT" REQUIRED="true"
      NUM_ROW="3" SHOW_IF="OPERATION_TYPE=='BUFFER'">
      <DEFAULT>8</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="CLIPPER_XMIN" FIELD="TEXT" REQUIRED="true"
      NUM_ROW="4" SHOW_IF="OPERATION_TYPE=='CLIPPER'">
    </PARAMETER>
    <PARAMETER NAME="CLIPPER_YMIN" FIELD="TEXT" REQUIRED="true"
      NUM_ROW="4" SHOW_IF="OPERATION_TYPE=='CLIPPER'">
    </PARAMETER>
    <PARAMETER NAME="CLIPPER_XMAX" FIELD="TEXT" REQUIRED="true"
      NUM_ROW="4" SHOW_IF="OPERATION_TYPE=='CLIPPER'">
    </PARAMETER>
    <PARAMETER NAME="CLIPPER_YMAX" FIELD="TEXT" REQUIRED="true"
      NUM_ROW="4" SHOW_IF="OPERATION_TYPE=='CLIPPER'">
    </PARAMETER>
    <PARAMETER NAME="SOURCE_CRS" FIELD="TEXT" REQUIRED="true"
      NUM_ROW="5" SHOW_IF="OPERATION_TYPE=='PROJ'">
    </PARAMETER>
    <PARAMETER NAME="TARGET_CRS" FIELD="TEXT" REQUIRED="true"
      NUM_ROW="5" SHOW_IF="OPERATION_TYPE=='PROJ'">
    </PARAMETER>
    <PARAMETER NAME="TOLERANCE" FIELD="TEXT" REQUIRED="true" NUM_ROW="6"
      SHOW_IF="OPERATION_TYPE=='DOUGLAS_PEUCKER' OR OPERATION_TYPE=='TOPOLOGY_PRESERVING'">
    </PARAMETER>
    <PARAMETER NAME="TX" FIELD="TEXT" REQUIRED="true"
      NUM_ROW="1">
      <DEFAULT>0</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="TY" FIELD="TEXT" REQUIRED="true"
      NUM_ROW="1">
      <DEFAULT>0</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="ROTATION" FIELD="TEXT" REQUIRED="true"
      NUM_ROW="2">
      <DEFAULT>0</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="AX" FIELD="TEXT" REQUIRED="true"
      NUM_ROW="3">
      <DEFAULT>1</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="AY" FIELD="TEXT" REQUIRED="true"
      NUM_ROW="3">
      <DEFAULT>1</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="SX" FIELD="TEXT" REQUIRED="true"
      NUM_ROW="4">
      <DEFAULT>0</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="SY" FIELD="TEXT" REQUIRED="true"
      NUM_ROW="4">
      <DEFAULT>0</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="USE_CENTROID_AS_ANCHOR" FIELD="CHECK" REQUIRED="true"
      NUM_ROW="5">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="LENIENT" FIELD="CHECK" REQUIRED="true"
      NUM_ROW="4">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="CRS_TRANSFORM" FIELD="CHECK" REQUIRED="true"
      NUM_ROW="5">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="CRS_TRANSFORM_FILE" FIELD="FILE" NUM_ROW="4" REQUIRED="false"
      REPOSITORY_VALUE="FILE_PATH" SHOW_IF="CRS_TRANSFORM == 'true'">
      <DEFAULT>"__COMP_DEFAULT_FILE_DIR__/.spatial/proj/epsg_operations.properties"</DEFAULT>
    </PARAMETER>
  </PARAMETERS>
  <ADVANCED_PARAMETERS>
	<PARAMETER NAME="IMPORT" FIELD="MEMO_IMPORT" REQUIRED="false" SHOW="false" NUM_ROW="1">
	  <DEFAULT>import org.talend.sdi.geometry.Geometry;</DEFAULT>
	</PARAMETER>   
  </ADVANCED_PARAMETERS>
  <CODEGENERATION>
    <IMPORTS>
      <IMPORT NAME="jts" MODULE="jts-1.12.jar" REQUIRED="true" />
      <IMPORT NAME="org.talend.sdi" MODULE="org.talend.sdi.jar"
        REQUIRED="true" />
      <IMPORT NAME="gt-opengis" MODULE="gt-opengis-8.5.jar"
        REQUIRED="true" />
      <IMPORT NAME="gt-main" MODULE="gt-main-8.5.jar"
        REQUIRED="true" />
      <IMPORT NAME="gt-metadata" MODULE="gt-metadata-8.5.jar"
        REQUIRED="true" />
      <IMPORT NAME="jsr-275" MODULE="jsr-275-1.0-beta-2.jar"
        REQUIRED="true" />
      <IMPORT NAME="hsqldb" MODULE="hsqldb-1.8.0.7.jar" REQUIRED="true" />
      <IMPORT NAME="vecmath" MODULE="vecmath-1.3.2.jar" REQUIRED="true" />
      <IMPORT NAME="gt-api" MODULE="gt-api-8.5.jar" REQUIRED="true" />
      <IMPORT NAME="gt-epsg-hsql" MODULE="gt-epsg-hsql-8.5.jar"
        REQUIRED="true" />
      <IMPORT NAME="gt-referencing" MODULE="gt-referencing-8.5.jar"
            REQUIRED="true" />
      <IMPORT NAME="jgridshift" MODULE="jgridshift-1.0.jar" REQUIRED="true"/>
      <IMPORT NAME="jai-codec" MODULE="jai_codec-1.1.3.jar" REQUIRED="true" />
      <IMPORT NAME="jai-core" MODULE="jai_core-1.1.3.jar" REQUIRED="true" />
    </IMPORTS>
  </CODEGENERATION>
  <RETURNS>
    <RETURN NAME="NB_FEATURE" TYPE="INTEGER" AVAILABILITY="AFTER" />
    <RETURN NAME="NB_FEATURE_VALID" TYPE="INTEGER" AVAILABILITY="AFTER" />
    <RETURN NAME="NB_FEATURE_INVALID" TYPE="INTEGER"
      AVAILABILITY="AFTER" />
  </RETURNS>
</COMPONENT>
