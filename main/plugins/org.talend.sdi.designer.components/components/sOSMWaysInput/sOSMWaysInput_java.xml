<COMPONENT>
  <HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.1" STATUS="ALPHA" COMPATIBILITY="ALL" AUTHOR="Fxp"
    RELEASE_DATE="20081028" STARTABLE="true">
    <SIGNATURE/>
  </HEADER>
  <FAMILIES>
    <FAMILY>Geo/File/Input/OpenStreetMap</FAMILY>
  </FAMILIES>
  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>
  <CONNECTORS>
    <CONNECTOR CTYPE="ITERATE" MAX_INPUT="1" MAX_OUTPUT="0"/>
    <CONNECTOR CTYPE="FLOW" MAX_OUTPUT="0" MAX_INPUT="0"/>
    <CONNECTOR CTYPE="FLOW" NAME="WAYS" MAX_OUTPUT="1" MAX_INPUT="0" BASE_SCHEMA="FLOW"/>
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
  </CONNECTORS>
  <PARAMETERS>
    <PARAMETER NAME="FILENAME" FIELD="FILE" NUM_ROW="1" REQUIRED="true" REPOSITORY_VALUE="FILE_PATH">
      <DEFAULT>"__COMP_DEFAULT_FILE_DIR__/in.osm"</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true" NUM_ROW="12" CONTEXT="WAYS"
      READONLY="true">
      <TABLE READONLY="true">
        <COLUMN NAME="id" TYPE="id_String" READONLY="true" CUSTOM="true"/>
        <COLUMN NAME="tags" TYPE="id_String" READONLY="true" CUSTOM="true"/>
        <COLUMN NAME="timestamp" TYPE="id_String" READONLY="true" CUSTOM="true"/>
        <COLUMN NAME="username" TYPE="id_String" READONLY="true" CUSTOM="true"/>
        <COLUMN NAME="way" TYPE="id_Geometry" READONLY="true" CUSTOM="true"/>
      </TABLE>
    </PARAMETER>
    
    <PARAMETER NAME="FAILONERROR" FIELD="CHECK" REQUIRED="true" NUM_ROW="4">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
  </PARAMETERS>
  <CODEGENERATION>
    <IMPORTS>
      <IMPORT NAME="josm" MODULE="josm.jar" REQUIRED="true"/>
      <IMPORT NAME="org.talend.sdi" MODULE="org.talend.sdi.jar" REQUIRED="true"/>
      <IMPORT NAME="jts" MODULE="jts-1.12.jar" REQUIRED="true" />
    </IMPORTS>
  </CODEGENERATION>
  <RETURNS>
    <RETURN NAME="OUTPUT_FILE" TYPE="id_String" AVAILABILITY="AFTER"/>
  </RETURNS>
</COMPONENT>
